(function() {
var ifr = document.createElement("iframe");
ifr.src = "https://cdn.doublemax.net/autoplayplayer/v5/autoplayContent.html?&ck=" + encodeURIComponent(ck) + "&p=" + p + "&videoUrl=" + v + "&keyTime=" + sec + "&videoW=300&videoH=250";
ifr.setAttribute("id", "perview-if");
ifr.setAttribute("width", 300);
ifr.setAttribute("height", 250);
ifr.setAttribute("scrolling", "no");
ifr.setAttribute("marginheight", "0");
ifr.setAttribute("marginwidth", "0");
ifr.setAttribute("allowtransparency", "true");
ifr.setAttribute("vspace", 0);
ifr.setAttribute("hspace", 0);
ifr.setAttribute("frameborder", 0);
document.body.appendChild(ifr);
var i = document.createElement("img");
	i.setAttribute("src", iu);
	i.setAttribute("width", 0);
	i.setAttribute("height", 0);
	i.setAttribute("style", "display:none;");
	document.body.appendChild(i); 
})();